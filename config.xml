<?xml version='1.0' encoding='utf-8'?>
<widget id="org.kchungradio.player" version="0.5.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>KCHUNG</name>
    <description>
        Listen to KCHUNG Radio 1630AM Chinatown, Los Angeles
    </description>
    <author email="engineering@kchungradio.org" href="http://www.kchungradio.org">
        KCHUNG Engineering Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="ios" spec="~4.1.1" />
    <preference name="deployment-target" value="7.0" />
    <preference name="Orientation" value="portrait" />
    <preference name="SuppressesLongPressGesture" value="true" />
    <!-- If you set following to true, test your app thoroughly since
         this disables onclick handlers, but plays nice with ontouchend. -->
    <!-- <preference name="Suppresses3DTouchGesture" value="true" /> -->
    <preference name="DisallowOverscroll" value="true" />
    <!--
    <preference name="" value="" />
    -->
</widget>
